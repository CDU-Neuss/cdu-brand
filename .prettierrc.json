{
  "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 140,
  "braceStyle": "1tbs",
  "tabWidth": 2,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
